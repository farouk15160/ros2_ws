<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="visiona">

    <!-- Import the gripper macro -->
    <xacro:include filename="./gripper.xacro" />
    <link name="world">
    </link>
    <link name="base">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
            <mass value="0.0" />
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0" />
        </inertial>
        <visual name="base">
            <origin xyz="0 0 0 " rpy="0 0 0 " />
            <geometry>
                <mesh filename="package://visiona_bridge/meshes/base.stl" scale="0.001 0.001 0.001" />
            </geometry>
            <!-- <material name="PLA">
                <color rgba="1.0 0.0 0.0 1.0" />
                <texture filename="" />
            </material> -->
        </visual>
        <collision>
            <origin xyz="0 0 0 " rpy="0 0 0 " />
            <geometry>
                <mesh filename="package://visiona_bridge/meshes/base.stl" scale="0.001 0.001 0.001" />
            </geometry>
        </collision>
    </link>
    <link name="base_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
            <mass value="0.0" />
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0" />
        </inertial>
        <visual name="base_link">
            <origin xyz="0 0 0.067 " rpy="0 0 0 " />
            <geometry>
                <mesh filename="package://visiona_bridge/meshes/base_link.stl"
                    scale="0.001 0.001 0.001" />
            </geometry>
            <!-- <material name="PLA">
                <color rgba="1.0 0.0 0.0 1.0" />
                <texture filename="" />
            </material> -->
        </visual>
        <collision>
            <origin xyz="0 0 0.067" rpy="0 0 0 " />
            <geometry>
                <mesh filename="package://visiona_bridge/meshes/base_link.stl"
                    scale="0.001 0.001 0.001" />
            </geometry>
        </collision>
    </link>
    <link name="link_1_shoulder">
        <inertial>
            <origin xyz="0 0 0 " rpy="0.0 0.0 0.0" />
            <mass value="0.0" />
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0" />
        </inertial>
        <visual name="link_1_shoulder">
            <origin xyz="0 0 0 " rpy="0 0 0 " />
            <geometry>
                <mesh filename="package://visiona_bridge/meshes/link_1_shoulder.stl"
                    scale="0.001 0.001 0.001" />
            </geometry>
            <!-- <material name="PLA">
                <color rgba="0.0 1.0 0.0 0.5" />
                <texture filename="" />
            </material> -->
        </visual>
        <collision>
            <origin xyz="0 0 0 " rpy="0 0 0 " />
            <geometry>
                <mesh filename="package://visiona_bridge/meshes/link_1_shoulder.stl"
                    scale="0.001 0.001 0.001" />
            </geometry>
        </collision>
    </link>
    <link name="link_2_elbow">
        <inertial>
            <origin xyz="0 0 0 " rpy="0.0 0.0 0.0" />
            <mass value="0.0" />
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0" />
        </inertial>
        <visual name="link_2_elbow">
            <origin xyz="0 0 0 " rpy="0 0 0 " />
            <geometry>
                <mesh filename="package://visiona_bridge/meshes/link_2_elbow.stl"
                    scale="0.001 0.001 0.001" />
            </geometry>
            <!-- <material name="PLA">
                <color rgba="0.0 1.0 0.0 0.5" />
                <texture filename="" />
            </material> -->
        </visual>
        <collision>
            <origin xyz="0 0 0 " rpy="0 0 0 " />
            <geometry>
                <mesh filename="package://visiona_bridge/meshes/link_2_elbow.stl"
                    scale="0.001 0.001 0.001" />
            </geometry>
        </collision>
    </link>
    <link name="link_3_wrist">
        <inertial>
            <origin xyz="0 0 0 " rpy="0.0 0.0 0.0" />
            <mass value="0.0" />
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0" />
        </inertial>
        <visual name="link_3_wrist">
            <origin xyz="0 0 0 " rpy="0 0 0 " />
            <geometry>
                <mesh filename="package://visiona_bridge/meshes/link_3_wrist.stl"
                    scale="0.001 0.001 0.001" />
            </geometry>
            <!-- <material name="PLA">
                <color rgba="0.0 1.0 0.0 0.5" />
                <texture filename="" />
            </material> -->
        </visual>
        <collision>
            <origin xyz="0 0 0 " rpy="0 0 0 " />
            <geometry>
                <mesh filename="package://visiona_bridge/meshes/link_3_wrist.stl"
                    scale="0.001 0.001 0.001" />
            </geometry>
        </collision>
    </link>


    <joint name="base_joint" type="fixed">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
        <parent link="world" />
        <child link="base" />
        <axis xyz="0.0 0.0 0.0" />
    </joint>

    <joint name="base_link_joint" type="revolute">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
        <parent link="base" />
        <child link="base_link" />
        <axis xyz="0.0 0.0 1" />
        <limit lower="-3.14" upper="3.14" effort="0.0" velocity="0.0" />
    </joint>

    <joint name="link_1_shoulder_joint" type="revolute">
        <origin xyz="-0.007 0 0.134 " rpy="0.0 0.0 0.0" />
        <parent link="base_link" />
        <child link="link_1_shoulder" />
        <axis xyz="1 0 0.0" />
        <limit lower="-1.52" upper="1.52" effort="0.0" velocity="0.0" />
    </joint>

    <joint name="link_2_elbow_joint" type="revolute">
        <origin xyz="0 0 0.185 " rpy="0 0 3.14" />
        <parent link="link_1_shoulder" />
        <child link="link_2_elbow" />
        <axis xyz="1 0 0.0" />
        <limit lower="-3.0" upper="3.0" effort="0.0" velocity="0.0" />
    </joint>

    <joint name="link_3_wrist_joint" type="revolute">
        <origin xyz="0 0 0.116 " rpy="0 0 3.14" />
        <parent link="link_2_elbow" />
        <child link="link_3_wrist" />
        <axis xyz="1 0 0.0" />
        <limit lower="-3.0" upper="3.0" effort="0.0" velocity="0.0" />
    </joint>
    <!-- <joint name="gripper_joint" type="revolute">
        <origin xyz="0 0 0.082 " rpy="1.5707963 0 0" />
        <parent link="link_3_wrist" />
        <child
            link="gripper_joint" />grip per_joint <axis xyz="0 1 0.0" />
        <limit lower="-3.0"
            upper="3.0" effort="0.0" velocity="0.0" />
    </joint> -->


    <xacro:gripper parent_link="link_3_wrist">

        <origin xyz="0 0 0.082" rpy="1.5707963 0 0" />
    </xacro:gripper>

</robot>

